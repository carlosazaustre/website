{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", ".next/**"]
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    },
    "globalDependencies": [
        ".env",
        "$GITHUB_TOKEN",
        "$NODE_ENV",
        "$NEXT_PUBLIC_GOOGLE_ANALYTICS"
    ]
}
  